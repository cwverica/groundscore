{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/searchNew.schema.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "title": {
      "type": "string",
      "maxLength": 50
    },
    "closestORI": {
      "type": "string",
      "minLength": 9,
      "maxLength": 9
    },
    "locationId": {
      "type": "integer",
      "minimum": 0
    },
    "userComments": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "username",
    "locationId",
    "closestORI"
  ]
}