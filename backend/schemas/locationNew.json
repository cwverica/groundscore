{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/locationNew.schema.json",
  "type": "object",
  "properties": {
    "lat": {
      "type": "number",
      "minimum": -90,
      "maximum": 90
    },
    "lng": {
      "type": "number",
      "minimum": -180,
      "maximum": 180
    },
    "county": {
      "type": "string",
      "minLength": 1
    },
    "state": {
      "type": "string",
      "minLength": 2,
      "maxLength": 2
    }
  },
  "additionalProperties": true,
  "required": [
    "lat",
    "lng",
    "state"
  ]
}